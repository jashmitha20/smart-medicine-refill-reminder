# Frontend Environment Configuration Template
# Copy this file to .env and update the values

# API Configuration
REACT_APP_API_URL=http://localhost:8080/api

# Application Configuration
REACT_APP_APP_NAME=Smart Medicine Refill System
REACT_APP_VERSION=1.0.0

# Development Configuration
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_ANALYTICS=false

# Mock API Mode (set to true to run frontend without backend)
REACT_APP_USE_MOCK_API=false
REACT_APP_MOCK_LOGIN_ALLOW_ANY=false

# Demo credentials (used in mock mode)
REACT_APP_DEMO_EMAIL=demo@example.com
REACT_APP_DEMO_PASSWORD=demo1234

# External Services
REACT_APP_PHARMACY_URL=https://www.1mg.com/search/all?name=

# Build Configuration
GENERATE_SOURCEMAP=true
REACT_APP_BUILD_DATE=2025-01-06
